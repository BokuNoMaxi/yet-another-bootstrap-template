{
  "name": "bokuno/yet-another-bootstrap-template",
  "type": "typo3-cms-extension",
  "description": "An extension that simply adds Bootstrap5 Elements to the page",
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "^12.4"
  },
  
  "autoload": {
    "psr-4": {
      "BokuNo\\YABT\\": "Classes"
    }
  },
  
  "replace": {
    "typo3-ter/yet-another-bootstrap-template": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/public",
      "extension-key": "yet_another_bootstrap_template"
    }
  }
}
